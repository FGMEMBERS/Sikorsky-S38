    <path>Aircraft/Sikorsky-S38/Models/Effects/wake.xml</path>
    <offsets>
      <x-m>  2.875 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -1.786 </z-m>
      <roll-deg>    0 </roll-deg>
      <pitch-deg> -45 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>

  <!-- Trainée flotteur gauche -->
