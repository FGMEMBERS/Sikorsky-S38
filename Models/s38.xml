      <x-m> -0.195 </x-m>
      <y-m> -3.790 </y-m>
      <z-m> -1.644 </z-m>
      <roll-deg>    0 </roll-deg>
      <pitch-deg> -45 </pitch-deg>
      <heading-deg> 0 </heading-deg>
    </offsets>
  </model>

  <!-- Trainée flotteur droite -->
