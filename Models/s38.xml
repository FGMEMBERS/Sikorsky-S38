  <!-- Les hélices --> 
  <animation>
    <name>HeliceGauche</name>
    <object-name>bolG</object-name>
    <object-name>heliceG</object-name>
    <object-name>propdiscG</object-name>
    <object-name>propblurG</object-name>
  </animation>

  <animation>
    <name>HeliceDroite</name>
    <object-name>bolD</object-name>
    <object-name>heliceD</object-name>
    <object-name>propdiscD</object-name>
    <object-name>propblurD</object-name>
  </animation>

  <!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdiscG</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblurG</object-name>
    <condition>
    <and>
      <greater-than>
        <property>engines/engine[0]/rpm</property>
        <value>600</value>
      </greater-than>
      <less-than>
        <property>engines/engine[0]/rpm</property>
        <value>1500.001</value>
      </less-than>
    </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>heliceG</object-name>
    <condition>
      <less-than>
        <property>engines/engine[0]/rpm</property>
	<value>1500</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>HeliceGauche</object-name>
    <property>engines/engine[0]/rpm</property>
    <factor> -1 </factor>
    <center>
      <x-m> -3.343 </x-m>
      <y-m> -1.588 </y-m>
      <z-m>  0.542 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Séléction de l'objet visible en fonction des rpm -->
  <animation>
    <type>select</type>
    <object-name>propdiscD</object-name>
    <condition>
      <greater-than>
        <property>engines/engine[1]/rpm</property>
        <value>1500</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblurD</object-name>
    <condition>
    <and>
      <greater-than>
        <property>engines/engine[1]/rpm</property>
        <value>600</value>
      </greater-than>
      <less-than>
        <property>engines/engine[1]/rpm</property>
        <value>1500.001</value>
      </less-than>
    </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>heliceD</object-name>
    <condition>
      <less-than>
        <property>engines/engine[1]/rpm</property>
	<value>1500</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>HeliceDroite</object-name>
    <property>engines/engine[1]/rpm</property>
    <factor> -1 </factor>
    <center>
      <x-m> -3.343 </x-m>
      <y-m>  1.588 </y-m>
      <z-m>  0.542 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Animations des ailerons -->
  <animation>
    <type>rotate</type>
    <object-name>aileronG</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <factor>15.0</factor>
    <center>
      <x-m> -0.649 </x-m>
      <y-m> -7.402 </y-m>
      <z-m>  1.395 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>aileronD</object-name>
    <property>surface-positions/right-aileron-pos-norm</property>
    <factor>15.0</factor>
    <center>
      <x-m> -0.649 </x-m>
      <y-m>  7.402 </y-m>
      <z-m>  1.395 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Gouverne de profondeur -->
  <animation>
    <type>rotate</type>
    <object-name>profondeur</object-name>
    <property>surface-positions/elevator-pos-norm</property>
    <factor> 25 </factor>
    <center>
      <x-m> 5.632 </x-m>
      <y-m> 0.000 </y-m>
      <z-m> 1.150 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y> 
      <z> 0 </z>
    </axis>
  </animation>

  <!-- Gouverne de direction -->
  <animation>
    <type>rotate</type>
    <object-name>directionG</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor> -25.0 </factor>
    <center>
      <x-m>  5.524 </x-m>
      <y-m> -1.588 </y-m>
      <z-m>  1.152 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  0 </y> 
      <z>  1 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>directionD</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor> -25.0 </factor>
    <center>
      <x-m> 5.524 </x-m>
      <y-m> 1.588 </y-m>
      <z-m> 1.152 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  0 </y> 
      <z>  1 </z>
    </axis>
  </animation>
  
  <!-- Rentrée des roues -->
  <animation>
    <name>TrainGauche</name>
    <object-name>axeG</object-name>
    <object-name>axeG2</object-name>
    <object-name>axeG3</object-name>
    <object-name>roueG</object-name>
  </animation>

  <animation>
    <name>RoueGauche</name>
    <object-name>roueG</object-name>
    <object-name>axeG2</object-name>
    <object-name>axeG3</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>RoueGauche</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> -90 </dep></entry>
      <entry><ind> 1 </ind><dep>   0 </dep></entry>
    </interpolation>
    <center>
      <x-m> -1.874 </x-m>
      <y-m> -1.588 </y-m>
      <z-m> -2.132 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>TrainGauche</object-name>
    <property>gear/gear[0]/position-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> 1 </dep></entry>
      <entry><ind> 1 </ind><dep> 0 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>axeG4</object-name>
    <property>gear/gear[0]/position-norm</property>
