      <x-m> -1.874 </x-m>
      <y-m>  1.588 </y-m>
      <z-m> -2.132 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>TrainDroit</object-name>
