      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind>  1 </ind><dep> -90 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.153 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.212 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
